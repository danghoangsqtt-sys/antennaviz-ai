{
  "name": "antennaviz-ai-advanced",
  "version": "2.0.0",
  "main": "dist-electron/main.js", 
  "description": "AntennaViz AI Advanced Edition",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder"
  },
  "build": {
    "appId": "com.antennaviz.ai",
    "productName": "AntennaViz AI",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "@google/genai": "^1.38.0",
    "@react-three/fiber": "^9.5.0",
    "@react-three/drei": "^10.7.7",
    "three": "^0.182.0",
    "lucide-react": "^0.563.0",
    "zustand": "^5.0.10",
    "@mediapipe/tasks-vision": "^0.10.32",
    "electron": "^40.1.0"
  },
  "devDependencies": {
    "electron": "^40.1.0",
    "electron-builder": "^24.13.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0"
  }
}